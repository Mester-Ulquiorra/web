---
import { getUrl } from "../scripts/functions";
import { getUserData } from "../scripts/userData";

const url = getUrl();
const userData = await getUserData(url);
---

<footer>Last fetched: {new Date(userData!.lastFetch).toString()}</footer>

<style>
    footer {
        text-align: center;
        opacity: 0.75;
        font-size: 0.75rem;
        padding: 0.5rem;
    }
</style>
